def dockerimage
 environment {
    registry = "amalguesmi/phpappauth"
    registryCredential = 'dockerhub'
    dockerImage = ''
  }
pipeline {
  agent any
  stages {
    stage('verify version') {
      steps {
        sh 'php --version'
      }
    }
   stage('Build image with docker') {
             steps{
                script{
                   
                   dockerImage = docker.build("amalguesmi/phpappauth:latest")
                    
                }
             }
                    
          }  
   stage('login') {
      steps {
      
        sh 'php index.php'
        
      }
    }
  }
}  

 
